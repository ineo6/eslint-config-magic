{
  "name": "eslint-config-magic",
  "version": "2.1.0",
  "description": "Magic ESLint 规则",
  "main": "index.js",
  "scripts": {
    "build": "ts-node ./scripts/build.ts",
    "add": "ts-node ./scripts/addMissingRules.ts",
    "dev:site": "cd ./site && fe dev",
    "build:site": "cd ./site && fe build",
    "test": "ts-node ./test/index.ts && npm run test:rulesCoverage",
    "test:rulesCoverage": "ts-node ./scripts/rulesCoverage.ts",
    "lint": "run-s eslint prettier",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx,.vue --ignore-pattern \"bad.*\" .",
    "prettier": "prettier -l \"./**/*\""
  },
  "homepage": "https://ineo6.github.io/eslint-config-magic/",
  "repository": {
    "type": "git",
    "url": "git@github.com:ineo6/eslint-config-magic.git"
  },
  "keywords": [
    "eslint",
    "eslintrc",
    "eslintconfig",
    "config",
    "javascript",
    "styleguide",
    "react",
    "typescript"
  ],
  "author": "ineo6",
  "contributors": [],
  "license": "MIT",
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "4.5.0",
    "@typescript-eslint/parser": "4.5.0",
    "babel-eslint": "10.1.0",
    "eslint": "^7.24.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-react": "7.21.5",
    "typescript": "^4.0.5"
  },
  "devDependencies": {
    "@fejs/fe": "^1.1.1",
    "@fejs/preset-react": "^1.0.0",
    "@types/cookie": "^0.4.0",
    "@types/doctrine": "^0.0.3",
    "@types/eslint": "^7.2.10",
    "@types/node": "^15.3.0",
    "@types/prettier": "^2.2.3",
    "@types/rimraf": "^3.0.0",
    "@types/xml-escape": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "babel-eslint": "10.1.0",
    "cookie": "^0.4.1",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-vue": "^7.9.0",
    "insert-tag": "^0.1.2",
    "mobi-plugin-color": "^1.0.0",
    "mobi.css": "^3.1.1",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0-beta.2",
    "prettier": "^2.3.0",
    "querystring": "^0.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-tooltip": "^4.2.19",
    "rimraf": "^3.0.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.4",
    "xml-escape": "^1.1.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
